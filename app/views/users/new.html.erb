
<head>
<%= csrf_meta_tags %>
</head>

<h1>Sign up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <form action="/users" method="post" name="users" accept-charset="UTF-8">

        username: <input id="user_username" name="user[username]" type="text"><br>
        email: <input id="user_email" name="user[email]" type="email"><br>
        password: <input id="user_password" name="user[password]" type="password"><br>
        password confirmation: <input id="user_password_confirmation" name="user[password_confirmation]" type="password"><br>
        <input name="commit" type="submit" value="Create user"><br>
        <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    </form>
  </div>
</div>